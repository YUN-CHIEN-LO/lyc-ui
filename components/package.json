{
  "name": "lyc-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/lyc-ui.umd.cjs",
  "module": "./dist/lyc-ui.js",
  "exports": {
    ".": {
      "import": "./dist/lyc-ui.js",
      "require": "./dist/lyc-ui.umd.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly && node ./src/scripts/build.cjs && cp -a ./src/themes/ ./dist/themes/",
    "types": "vue-tsc ",
    "preview": "vite preview",
    "clean": "npm cache clean -force"
  },
  "dependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@types/node": "^20.8.4",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "fs": "^0.0.1-security",
    "fs-extra": "^11.1.1",
    "sass": "^1.69.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vue-tsc": "^1.8.5"
  }
}
